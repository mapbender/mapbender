{% for set_id, label in dimensionsets %}
  <div id="{{ set_id }}" class="dimensionset row no-gutters">
    <div class="col-xs-4 col-lg-3 dimensionset-title">{{ label | humanize }}</div>
    <div class="col-xs-4 col-lg-6 slider-wrap"><div class="mb-slider" id="dimensionset-{{ set_id }}"></div></div>
    <div class="col-xs-4 col-lg-3 dimensionset-value"></div>
  </div>
{% endfor %}
