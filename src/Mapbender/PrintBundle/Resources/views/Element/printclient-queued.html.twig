    <div class="tab-container">
        <ul class="btn-group btn-group-sm list-unstyled">
            <li class="btn btn-default active"><a href="{{ '#' ~ id ~ '-settings' }}">{{ 'mb.print.printclient.tab.settings' | trans }}</a></li>
            <li class="btn btn-default"><a href="{{ '#' ~ id ~ '-job-list' }}">{{ 'mb.print.printclient.tab.joblist' | trans }}</a></li>
        </ul>
        <div id="{{id ~ '-settings' }}" class="print-form">
            {% include settingsTemplate %}
        </div >
        <div id="{{id ~ '-job-list' }}" class="job-list">
            <table class="table table-striped table-condensed">
                <thead>
                <tr>
                    <th>{{ 'mb.print.printclient.joblist.header.id' | trans }}</th>
                    <th>{{ 'mb.print.printclient.joblist.header.ctime' | trans }}</th>
                    <th>{{ 'mb.print.printclient.joblist.header.status' | trans }}</th>
                    <th class="interface"></th>
                </tr>
                </thead>
                <tfoot></tfoot>
                <tbody>
                </tbody>
            </table>
        </div>
        <iframe class="hidden" name="{{ submitFrameName }}"></iframe>
    </div>
